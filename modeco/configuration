#!/usr/bin/env bash

##### START MOULE CONFIGURATION KEYVALUES #####
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_FILEPATHS="/hostfs/var/lib/docker/containers/*/*.json"
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_FILEPATHS_MAPPING=",'/var/lib/docker/containers:/hostfs/var/lib/docker/containers:rw'"
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_HOSTS="kafka:9092"
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_USERNAME="kafka"
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_PASSWORD="password"
export MODECO_INGEST_FILE_SASL_ENVIRONMENT_OUTPUT_CONFIG_KAFKA_TOPIC="ingest_file"
##### END MOULE CONFIGURATION KEYVALUES #####
